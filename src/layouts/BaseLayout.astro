---
// propsの型定義（任意だが推奨）
interface Props {
	title?: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="ja">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	</head><p>{title}</p>

	<body>
		<slot />
	</body>
</html>
